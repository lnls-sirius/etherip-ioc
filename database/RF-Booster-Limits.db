    field(DOL,  "$(P):Set_Temp_0_ CP")
    field(DESC, "Upper limits from PLC")
    field(EGU,  "C")
}

record(calcout, "BO-05D:RF-P5Cav:Cylin5T-Mon_LOLO"){
    field(CALC, "A+0.5")
    field(INPA, "BO-05D:RF-P5Cav:Cylin5TLowerLimit-Cte CP")
    field(OUT,  "BO-05D:RF-P5Cav:Cylin5T-Mon.LOLO PP")
}

record(calcout, "BO-05D:RF-P5Cav:Cylin5T-Mon_LOW"){
    field(CALC, "A+1.0")
    field(INPA, "BO-05D:RF-P5Cav:Cylin5TLowerLimit-Cte CP")
    field(OUT,  "BO-05D:RF-P5Cav:Cylin5T-Mon.LOW PP")
}
